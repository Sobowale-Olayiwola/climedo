{
  "name": "climedo",
  "version": "1.0.0",
  "description": "IAM",
  "main": "./src/server.js",
  "scripts": {
    "start": "npm run seed && npm run build && pm2-runtime dist/server.js",
    "seed": "./node_modules/.bin/babel ./src -d seed-dist && node seed-dist/data/seeder.js && npm run seed:clean",
    "seed:clean": "rimraf seed-dist",
    "dev": "nodemon --exec babel-node src/server.js --experimental-specifier-resolution=node",
    "build": "./node_modules/.bin/babel src -d dist ",
    "test": "./node_modules/.bin/mocha 'test/**/*.js' --recursive --require @babel/register",
    "test:coverage": "nyc --reporter=html --check-coverage=true ./node_modules/.bin/mocha 'test/**/*.js' --recursive --require @babel/register"
  },
  "nyc": {
    "exclude": "src/repository"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "express": "^4.18.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "hpp": "^0.2.3",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.4",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "password-validator": "^5.3.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.16.4",
    "@babel/register": "^7.10.5",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.3.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^14.0.0"
  }
}
